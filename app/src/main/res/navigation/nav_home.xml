<?xml version="1.0" encoding="utf-8"?><navigation xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:id="@+id/nav_home"    app:startDestination="@id/homeFragment">    <fragment        android:id="@+id/homeFragment"        android:name="ir.gonabad.taskmanagment.presentation.home.HomeFragment"        android:label="HomeFragment" >        <action            android:id="@+id/action_homeFragment_to_addCategoryFragment"            app:destination="@id/addCategoryFragment" />        <action            android:id="@+id/action_homeFragment_to_taskListFragment"            app:destination="@id/taskListFragment" />        <action            android:id="@+id/action_homeFragment_to_settingAdminFragment"            app:destination="@id/settingAdminFragment" />    </fragment>    <fragment        android:id="@+id/addCategoryFragment"        android:name="ir.gonabad.taskmanagment.presentation.addCategory.AddCategoryFragment"        android:label="AddCategoryFragment" />    <fragment        android:id="@+id/taskListFragment"        android:name="ir.gonabad.taskmanagment.presentation.taskList.TaskListFragment"        android:label="TaskListFragment" >        <action            android:id="@+id/action_taskListFragment_to_manageTaskFragment"            app:destination="@id/manageTaskFragment" />    </fragment>    <fragment        android:id="@+id/manageTaskFragment"        android:name="ir.gonabad.taskmanagment.presentation.manageTask.ManageTaskFragment"        android:label="ManageTaskFragment" />    <fragment        android:id="@+id/settingAdminFragment"        android:name="ir.gonabad.taskmanagment.presentation.settingAdmin.SettingAdminFragment"        android:label="SettingAdminFragment" >        <action            android:id="@+id/action_settingAdminFragment_to_addUserFragment"            app:destination="@id/addUserFragment" />        <action            android:id="@+id/action_settingAdminFragment_to_userListFragment"            app:destination="@id/userListFragment" />    </fragment>    <fragment        android:id="@+id/addUserFragment"        android:name="ir.gonabad.taskmanagment.presentation.addUser.AddUserFragment"        android:label="AddUserFragment" />    <fragment        android:id="@+id/userListFragment"        android:name="ir.gonabad.taskmanagment.presentation.userList.UserListFragment"        android:label="UserListFragment" /></navigation>